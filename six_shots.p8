-- @init
function six_init()
	
-- @particle array
	p={}
	
	particle={
		x,y,
		vx,vy,
		r,lx,ly,
		t=25,
		visible=true,
	
		new=function(s,tbl)
			tbl=tbl or {}
			setmetatable(tbl,{
			__index=s
			})
			return tbl
		end,
	
		update=function(s)
--apply forces
			if s.t>0 then
				s.x+=s.vx*0.4
				s.y-=s.vy*0.4
				s.vx*=0.97
				s.vy-=0.05
				else
				s.visible=false
				
			end
		
			s.t-=1
	end,
	
	draw=function(s)
		circfill(s.x,s.y,s.r,7)
	end
	
	}
	
-- @bullet
	bullet={
		x,y,
		angle,
		mx,my,	-- mouse offset
		speed=1.1,
		visible=true,
		
		new=function(s,tbl)
			tbl=tbl or {}
			setmetatable(tbl,{
				__index=s
			})
			return tbl
		end,
		
		init=function(s)
			mag=s.mx*s.mx+s.my*s.my
			mag=sqrt(mag)
			s.mx=s.mx/mag
			s.my=s.my/mag
		end,
		
		update=function(s)
-- need to normalize vector
-- for constant speed
-- x/x^2+y^2 (magnitude)
			if map_collision(s,2)!=true 
			and s.x>-10
			and s.x<130
			and s.y>-10
			and s.y<130 then
				s.x+=s.mx*s.speed
				s.y+=s.my*s.speed
			else
				s.visible=false
			end
			if s.visible!=true 
			and s.x>0
			and s.x<128
			and s.y>0
			and s.y<128 then
				sfx(2)
			
			
				add(p,particle:new({
				x=s.x,y=s.y,
				r=rnd(2),
				vx=rnd(1),
				vy=rnd(1)
				}))
				
				add(p,particle:new({
				x=s.x,y=s.y,
				r=rnd(2),
				vx=rnd(1)*-1,
				vy=rnd(1)
				}))
				
				add(p,particle:new({
				x=s.x,y=s.y,
				r=rnd(2),
				vx=rnd(2)*-1,
				vy=rnd(1)
				}))
				
			end
		end,
		
		draw=function(s)
			if s.visible then
				spr(112,s.x-4,s.y-4)
			end
		end
	}

-- bullet array
	b={}

	player={
		health=100,
		stamina=100,
		x=64,y=64,
		lx,ly,
		dspeed=0.18,
		speed=0.18,
		sprnt=1.5,
		angle,
		dir=0,
		ammo=6,
		ammo_cap=6,
		shoot_delay=0,--
		
		update=function(s)

-- sprint
			if btn(‚ùé) 
			and s.stamina>0 then
				s.speed=s.dspeed*s.sprnt
				s.stamina-=0.5
			else
				if s.stamina<100 then
					s.stamina+=0.4
				end
				s.speed=s.dspeed
			end

-- input	
			if btn(‚¨ÖÔ∏è) and s.x>5 then
				s.x-=s.speed	end
			if btn(‚û°Ô∏è) and s.x<123 then
				s.x+=s.speed	end
			if btn(‚¨ÜÔ∏è) and s.y>5 then
				s.y-=s.speed	end
			if btn(‚¨áÔ∏è) and s.y<123 then
				s.y+=s.speed	end
				
-- angle to spr
			local dx=mouse.x-s.x
			local dy=mouse.y-s.y
			local l=0.0625--half of 1/8
			s.angle=atan2(dx,dy)
			if s.angle>0.25-l 
			and s.angle<0.25+l then
				s.dir=0
			elseif s.angle>0.375-l
			and s.angle<0.375+l then
				s.dir=7
			elseif s.angle>0.5-l
			and s.angle<0.5+l then
				s.dir=6
			elseif s.angle>0.625-l
			and s.angle<0.625+l then
				s.dir=5
			elseif s.angle>0.75-l
			and s.angle<0.75+l then
				s.dir=4
			elseif s.angle>0.875-l
			and s.angle<0.875+l then
				s.dir=3
			elseif s.angle>0.875+l
			or s.angle<0+l then
				s.dir=2
			elseif s.angle>0.125-l
			and s.angle<0.125+l then
				s.dir=1
			end
			
			-- reload
			if btn(üÖæÔ∏è) 
			and s.ammo<s.ammo_cap then
				s.ammo+=0.03
			end
		
-- shooting
			if mouse.clicked 
			and s.ammo>=1 
			and s.shoot_delay<0 
			and btn(üÖæÔ∏è)!=true then
				sfx(1)
				s.ammo-=1
				
				add(b,bullet:new({
				x=s.x,y=s.y,
				angle=s.angle,
				mx=dx,my=dy
				}))
			
				b[#b]:init()
			
				s.shoot_delay=25
			elseif mouse.clicked
			and s.ammo<1
			and s.shoot_delay<0
			and btn(üÖæÔ∏è)!=true then
				sfx(3)
				s.shoot_delay=25
			end
			
			s.shoot_delay-=1
		
			if map_collision(s,2) then
				s.x=s.lx
				s.y=s.ly
			end
		
			s.lx=s.x
			s.ly=s.y
		end,
		
		draw=function(s)
--			print(s.angle,0,0,9)
			spr(s.dir,s.x-4,s.y-4)
			
-- hud stuff
			local t=97
			for i=1,6 do
				if i<=s.ammo then
					t=96
				else
					t=97
				end
				spr(t,-6+(i*8),3)
			end
			
			rectfill(53,1,126,6,5)
			for x=1,9 do
				if x<s.health/9 then
					spr(62,46+x*8)
				end
			end
			
			rectfill(53,8,126,13,5)
			for x=1,9 do
				if x<s.stamina/9 then
					spr(61,46+x*8,7)
				end
			end
			
		end
	}
end

-- @update
function six_update()
	player:update()
	
	for i=1,#b do
		b[i]:update()
	end
	for i=1,#b do
		if i<=#b then 
			if b[i].visible!=true then
				deli(b,i)
			end
		end
	end
	
	for i=1,#p do
		p[i]:update()
	end
	for i=1,#p do
		if i<=#p then
			if p[i].visible==false then
				deli(p,i)
			end
		end
	end
end

-- @draw
function six_draw()
	map()
	for i=1,#b do
		b[i]:draw()
	end
	player:draw()
	
	for i=1,#p do
		p[i]:draw()
	end
end
