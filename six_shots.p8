-- @init
function enemy_init()

	enemy={
		pos,
		weapon,-- pistol or hands?
		angle,
		dir=8,
		ammo=6,
		ptbl=vec(0,0), -- pathfinding nodes
		pcool=60,	-- path cooldown
		pstep=1,
		
		new=function(s,tbl)
			local tbl=tbl or {}
			setmetatable(tbl,{
			__index=s
			})
			return tbl
		end,
		
		update_path=function(s)
			local t=vec(flr(player.x/8),flr(player.y/8))
			local pos=vec(flr(s.pos.x/8),flr(s.pos.y/8))

			s.ptbl=find_path(pos,t)
			
		end,
		
		step_path=function(s)
			if s.pstep>#s.ptbl then
				s:update_path()
			else
				local off=vec(4,4)
				s.pos=s.ptbl[s.pstep]+off
				s.pstep+=1
			end
		end,
		
		update=function(s)
		
-- spr direction
			if s.pstep<#s.ptbl then
				dpos=s.ptbl[s.pstep+1]-s.pos
							
				if dpos.x>10
				and in_range(dpos.y,-5,5) then
					s.dir=10
				elseif	dpos.x<-10
				and in_range(dpos.y,-5,5) then
					s.dir=14
				elseif dpos.y>10
				and in_range(dpos.x,-5,5) then
					s.dir=12
				elseif	dpos.y<-10
				and in_range(dpos.x,-5,5) then
					s.dir=8
				elseif dpos.x>10
				and dpos.y==-20 then
					s.dir=9
				elseif	dpos.x<-10
				and dpos.y then
					s.dir=14
				elseif dpos.y==20
				and dpos.x>10 then
					s.dir=11
				elseif	dpos.y<-10
				and in_range(dpos.x,-5,5) then
					s.dir=8
				end
			end
			
			if s.pcool<1 then
				s:update_path()
				s.pcool=60
				s.pstep=1
			end
			
			if s.pcool%30==0 then
				s:step_path()
			end
			
			s.pcool-=1
		end,
		
		draw=function(s)
			spr(s.dir,s.pos.x-4,s.pos.y-4)
			for i=1,#s.ptbl do
				spr(60,s.ptbl[i].x,s.ptbl[i].y)
			end
		end
		
	}

	etbl={}	--tbl of all enemy

	add(etbl,enemy:new{
		pos=vec(32,64)
		
	})
	
	etbl[1]:update_path()

end

-- @update
function enemy_update()
	for i=1,#etbl do
		etbl[i]:update()
	end
end

-- @draw
function enemy_draw()
	for i=1,#etbl do
		etbl[i]:draw()
	end
end


