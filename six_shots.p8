	player:update()
	
-- enemy pathfinding
	a_star_update()
	
	for i=1,#b do
		b[i]:update()
	end
	for i=1,#b do
		if i<=#b then 
			if b[i].visible!=true then
				deli(b,i)
			end
		end
	end
	
	for i=1,#p do
		p[i]:update()
	end
	for i=1,#p do
